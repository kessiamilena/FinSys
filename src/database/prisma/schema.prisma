generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url = env("DATABASE_URL")
}

model transactions {
    id  String  @id @default(auto()) @map("_id") @db.ObjectId
    created_at  DateTime @db.Date
    title       String 
    type        String
    category    String
    amount      Float
}